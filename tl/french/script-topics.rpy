    # m "Quiet forest paths..."
    m "Chemins de forêt silencieux..."

# game/script-topics.rpy:3467
translate french monika_ks_lilly_2ce174d5:

    # m 2dubsu "Romantic moments against a setting sun..."
    m 2dubsu "Des moments romantiques contre un soleil couchant..."

